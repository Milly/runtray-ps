{
    "$schema": "https://raw.githubusercontent.com/Milly/runtray-ps/v1.1.0/schemas/config.schema.json",
    "name": "sleep5",
    "description": "sleep 5 seconds",
    "executable": "powershell.exe",
    "arguments": [
        "-NoProfile",
        "-ExecutionPolicy", "RemoteSigned",
        "-Command", "&{for($i=5;0 -lt $i;--$i){echo $i;sleep 1}echo 'done'}"
    ]
}
